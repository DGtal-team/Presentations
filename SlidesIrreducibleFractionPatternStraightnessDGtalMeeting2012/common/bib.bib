  TITLE = {An Inductive Characterization of Matching in Binding
                  Bigraphs},
  JOURNAL = {To appear},
  YEAR = 2008,
  ABSTRACT = {We analyze the matching problem for bigraphs. In
                  particular, we present a sound and complete
                  inductive characterization of matching in bigraphs
                  with binding. Our results yield a specification for
                  a provably correct matching algorithm, as needed by
                  our prototype tool implementing bigraphical reactive
                  systems.},
  PDF = {http://www.itu.dk/~panic/Papers/DamgaardEtAl-2008-InductiveCharacterizationMatching.pdf}
}

@ARTICLE{Glenstrup:Matching,
